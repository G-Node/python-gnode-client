{
    "version": 1,
    "disable_existing_loggers": true,
    "formatters": {
        "verbose": {
            "format": "%(levelname)s %(asctime)s %(message)s"
        },
        "simple": {
            "format": "%(levelname)s %(message)s"
        }
    },
    "handlers": {
        "console":{
            "level":"DEBUG",
            "class":"logging.StreamHandler",
            "formatter": "simple"
        },
        "file": {
            "level": "INFO",
            "class": "logging.FileHandler",
            "filename": "gnode.log",
            "formatter": "verbose"
        }
    },
    "loggers": {
        "gnode": {
            "handlers":["file"],
            "propagate": true,
            "level": "INFO"
        }
    }
}
